{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:@tanstack/eslint-plugin-query", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "arrow-parens": [1, "as-needed"],
    "no-unused-vars": [
      2,
      {
        "argsIgnorePattern": "res|next|^err"
      }
    ],
    "arrow-body-style": [1, "as-needed"]
  }
}
